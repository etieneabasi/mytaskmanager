    "homepage": "https://github.com/etieneabasi/mytaskmanager",
    "keywords": ["Laravel", "MyTaskManager"],
    "require": {
        "illuminate/support": "~5",
        "uxweb/sweet-alert":"~2"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Etieneabasi\\MyTaskManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Etieneabasi\\MyTaskManager\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Etieneabasi\\MyTaskManager\\MyTaskManagerServiceProvider"
            ],
            "aliases": {
                "MyTaskManager": "Etieneabasi\\MyTaskManager\\Facades\\MyTaskManager"
            }
        }
    }
}
